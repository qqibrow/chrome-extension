{
  "manifest_version": 2,

  "name": "stackoverflow oauth",
  "description": "a demo to show stackoverflow oauth",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "identity",
    "https://stackexchange.com/*"
  ],
  "background": {
    "scripts": ["jquery-2.1.4.min.js", "background.js"]
  }
}
